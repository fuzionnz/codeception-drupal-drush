{
    "name": "guncha25/codecept-drupal-drush",
    "description": "A Codeception module to allow drush commands to be run during tests.",
    "homepage": "https://github.com/ixis/codeception-drupal-drush",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Rigby",
            "email": "andy.rigby@ixis.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Codeception\\Module\\": "src/"
        }
